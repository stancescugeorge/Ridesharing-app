<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/back_sim_stroke"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/tv_adapter_istoric_data"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:gravity="center"
        android:text="@string/text_data"
        android:textSize="17dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

    <ImageView
        android:id="@+id/iv_adapter_istoric_plecare"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="5dp"
        android:src="@drawable/plecare"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/iv_adapter_istoric_directie"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintTop_toBottomOf="@+id/tv_adapter_istoric_data"/>

    <ImageView
        android:id="@+id/iv_adapter_istoric_directie"
        android:layout_width="60dp"
        android:layout_height="30dp"
        android:layout_marginTop="10dp"
        android:src="@drawable/directie"
        app:layout_constraintEnd_toStartOf="@+id/iv_adapter_istoric_destinatie"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/iv_adapter_istoric_plecare"
        app:layout_constraintTop_toBottomOf="@id/tv_adapter_istoric_data" />

    <ImageView
        android:id="@+id/iv_adapter_istoric_destinatie"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="5dp"
        android:src="@drawable/destinatie"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/iv_adapter_istoric_directie"
        app:layout_constraintTop_toBottomOf="@id/tv_adapter_istoric_data"/>

    <TextView
        android:id="@+id/tv_adapter_istoric_plecare"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:gravity="center"
        android:text="@string/text_locatie_de_plecare"
        android:textSize="15dp"
        app:layout_constraintEnd_toStartOf="@+id/iv_adapter_istoric_directie"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_adapter_istoric_plecare"/>

    <TextView
        android:id="@+id/tv_adapter_istoric_destinatie"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:gravity="center"
        android:text="@string/text_locatie_de_destinatie"
        android:textSize="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/iv_adapter_istoric_directie"
        app:layout_constraintTop_toBottomOf="@+id/iv_adapter_istoric_plecare"/>

    <ImageView
        android:id="@+id/iv_adapter_istoric_ora"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="15dp"
        android:src="@drawable/ora"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_adapter_istoric_plecare"
        app:layout_constraintBottom_toBottomOf="parent"/>

    <TextView
        android:id="@+id/tv_adapter_istoric_ora"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:gravity="center"
        android:text="@string/text_hh_mm"
        android:textSize="15dp"
        app:layout_constraintEnd_toStartOf="@+id/iv_adapter_istoric_directie"
        app:layout_constraintStart_toStartOf="@+id/iv_adapter_istoric_ora"
        app:layout_constraintTop_toBottomOf="@+id/tv_adapter_istoric_plecare"/>

    <ImageView
        android:id="@+id/iv_adapter_istoric_durata"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:src="@drawable/durata"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/iv_adapter_istoric_directie"
        app:layout_constraintTop_toBottomOf="@+id/tv_adapter_istoric_destinatie"
        app:layout_constraintBottom_toBottomOf="parent"/>

    <TextView
        android:id="@+id/tv_adapter_istoric_durata"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="13dp"
        android:gravity="center"
        android:text="@string/text_durata_minute"
        android:textSize="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/iv_adapter_istoric_durata"
        app:layout_constraintTop_toBottomOf="@+id/tv_adapter_istoric_destinatie"
        />

</androidx.constraintlayout.widget.ConstraintLayout>